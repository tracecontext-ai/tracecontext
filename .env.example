# TraceContext Environment Variables
# Copy this file to .env and fill in your values.

# Required: OpenAI API key for GPT models
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Orchestrator URL (default: localhost)
ORCHESTRATOR_URL=http://localhost:8000

# Optional: PostgreSQL connection (requires pip install "tracecontext[db]")
# DATABASE_URL=postgresql://user:password@localhost:5432/tracecontext

# Optional: Redis connection (requires pip install "tracecontext[db]")
# REDIS_HOST=localhost
# REDIS_PORT=6379
